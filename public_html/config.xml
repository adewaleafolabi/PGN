<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.wale.example"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>Plugin</name>

    <description>
        An example for phonegap build docs. 
    </description>

    <author href="https://build.phonegap.com" email="support@phonegap.com">
        Hardeep Shoker 
    </author>
    
    <!--    <preference name="phonegap-version" value="2.9.0" />  all: current version of PhoneGap -->
    
    <preference name="android-minSdkVersion" value="14" /> <!-- android: MIN SDK version supported on the target device. MAX version is blank by default. -->
    <gap:splash src="ldpi.png" gap:platform="android" gap:density="ldpi" />
    <gap:splash src="mdpi.png" gap:platform="android" gap:density="mdpi" />
    <gap:splash src="hdpi.png" gap:platform="android" gap:density="hdpi" />
    <gap:splash src="xhdpi.png" gap:platform="android" gap:density="xhdpi" />
    <gap:splash src="xxhdpi.png" gap:platform="android" gap:density="xxhdpi" />

    <gap:plugin name="org.apache.cordova.camera" />
    <gap:plugin name="org.apache.cordova.media-capture" />
    <gap:plugin name="org.apache.cordova.device" />
    <gap:plugin name="hu.dpal.phonegap.plugins.pindialog" version="0.1.2" />
    <gap:plugin name="hu.dpal.phonegap.plugins.contactnumberpicker" version="0.1.1" />
    <gap:plugin name="com.dileep.plugins.datepicker" version="0.0.1" />
    <gap:platform name="android" />


</widget>
